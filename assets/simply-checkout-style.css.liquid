.product-table [data-product-type="hidden"],.product-table .product__description .product__description__property {display: none;}.cod_banner img{padding-right:8px}.cod_banner{margin:0;text-align:left;line-height:16px;font-size:14px;color:#000;margin-bottom:10px;padding:10px;background:#FFFFE0;border-radius:5px;font-weight:bold;display: flex;align-items: center;}@media (min-width: 768px){.cod_banner{margin-top:-15px}}.loading_me{position:fixed;top:0;left:0;right:0;bottom:0;background:rgba(0,0,0,0.54);display:none;z-index:1000}.loading_me .img{position:fixed;top:50%;left:50%;-moz-transform:translate(-50%, -50%);-o-transform:translate(-50%, -50%);-ms-transform:translate(-50%, -50%);-webkit-transform:translate(-50%, -50%);transform:translate(-50%, -50%);background:#000;padding:10px;border-radius:10px;line-height:1;color:#fff}.overflow_hidden{overflow:hidden}.c_topbar img{display:block}@media (max-width: 767px){.c_topbar img.xs-hide{display:none}}@media (min-width: 768px){.c_topbar img.xs-show{display:none}}.c_topbar .checkout_logo{position:absolute;display:block;top:13%;left:5%}